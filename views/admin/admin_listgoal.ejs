<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('partials/head.ejs') %>
   <link rel="stylesheet" href="/assets/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
  <link rel="stylesheet" href="/assets/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
  <link rel="stylesheet" href="/assets/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
</head>
<body class="hold-transition sidebar-mini layout-fixed">

   <div class="wrapper">

          <!-- Preloader -->
          <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="/assets/dist/img/AdminLTELogo.png" alt="AdminLTELogo" height="60" width="60">
          </div>
          
          <%- include('partials/top.ejs') %>
          
          <%- include('partials/sidebar.ejs') %>
          
          <div class="content-wrapper">
                    <!-- Content Header (Page header) -->
                    <div class="content-header">
 <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">Users Golas</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Golas </li>
            </ol><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    
     <section class="content">
      <div class="container-fluid">
     
           <div class="card">
              <div class="card-header">
                <h3 class="card-title">List of Users Golas</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <table id="example1" class="table table-bordered table-striped">
                  <thead>
                  <tr>
                    <th>Date</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>No of Goals</th>
                    <th>View Goal</th>
                    <th>Delete</th>
                  </tr>
                  </thead>
                  <tbody>
                       <% if(data.length){
                            for(var i = 0; i< data.length; i++) {%>
                    <tr>
                        <td><%= data[i].gdate%></td>
                        <td><%= data[i].uname%></td>
                        <td><%= data[i].uemail%></td>
                        <td><%= data[i].noofgoals%></td>
                        <td><a href="/admin_usergoal/<%= data[i].group_id%>">View Goal</a></td>
                        <td><a href="/deleteuserallgoal/<%= data[i].group_id%>" onclick="return confirm('Are you sure you want to delete this goal?');">Delete</a></td>
                    </tr>
                    <% } }else{ %>
                    <tr>
                          <td colspan="6">No Record Found</td>
                    </tr>
                    <% } %> 
                    </tbody>
                  <tfoot>
                  <tr>
                   <th>Date</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>No of Goals</th>
                    <th>View Goal</th>
                    <th>Delete</th>
                  </tr>
                  </tfoot>
                </table>
              
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
      </div>
      </div>
     </section> 
      </div>     
      </div>
                    <!-- /.content-header -->
          </div>            
          <%- include('partials/footer.ejs') %>
</div>


 <div class="modal fade" id="modal_addaudio">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h4 class="modal-title">Add New Audio</h4>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
               <div class="form-group">
               	 <label for="exampleInputEmail1">Audio Title</label>
                   	<div class="custom-file1">
                          <input type="text" name="taudio" class="form-control" id="taudio">
                    </div>
                </div>
               <div class="form-group">
               	 <label for="exampleInputEmail1">Upload Audio File</label>
                   	<div class="custom-file1">
                          <input type="file" name="uaudio" class="form-control" id="uaudio">
                    </div>
                </div>
                <div class="form-group">
               	 <label for="exampleInputEmail1">Upload Audio Image</label>
                   	<div class="custom-file1">
                          <input type="file" name="iaudio" class="form-control" id="iaudio">
                    </div>
                </div>
                <div class="form-group">
               	 <label for="exampleInputEmail1">Audio Category</label>
                   	<div class="custom-file1">
                          <select name="caudio" class="form-control">
                              <option value="">Select Category</option>
                              <option>Category1</option>
                              <option>Category2</option>
                              <option>Category3</option>
                              <option>Category4</option>
                              <option>Category5</option>
                          </select>
                    </div>
                </div>
                <div class="form-group">
               	 <label for="exampleInputEmail1">Gender</label>
               	 <div class="row">
               	 <div class="col-sm-4">
                   	<div class="form-check">
                          <input class="form-check-input" type="radio" name="gaudio">
                          <label class="form-check-label">Male</label>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-check">
                          <input class="form-check-input" type="radio" name="gaudio">
                          <label class="form-check-label">Female</label>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-check">
                          <input class="form-check-input" type="radio" name="gaudio">
                          <label class="form-check-label">Both</label>
                    </div>
                </div>    
                </div>
                </div>
            </div>
            <div class="modal-footer justify-content-between">
               <button type="button" class="btn btn-primary">Submit</button>
            </div>
          </div>
          <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
      </div>
      <!-- /.modal -->

